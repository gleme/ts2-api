{
  "name": "express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run build -- --watch\" \"nodemon --exec babel-node src/bin/www.js\"",
    "build": "babel src/ --out-dir build/ --source-maps inline --copy-files --ignore spec.js",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint src/*/*.js --config .eslintrc.js",
    "test": "npx mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "npx mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf build",
    "clean:all": "rimraf build node_modules"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "dependencies": {
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "lodash": "^4.17.11",
    "log4js": "^4.1.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.5",
    "sequelize": "^5.2.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0"
  }
}
